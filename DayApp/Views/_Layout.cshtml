@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Gopi</title>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Css/Main.css" rel="stylesheet" />
    <link href="~/Content/Css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Css/bootstrap.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="~/Content/Js/bootstrap.min.js"></script>
</head>
<body>
    <nav role="navigation" class="navbar navbar-inverse">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" data-target="#navbarCollapse" data-toggle="collapse" class="navbar-toggle">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="#" class="navbar-brand">Gopi.Ch</a>
        </div>
        <!-- Collection of nav links, forms, and other content for toggling -->
        <div id="navbarCollapse" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active gopich"><a href="/Home/Index" id="navLink1">Home</a></li>
                <li class="gopich"><a href="/Home/Photos" id="navLink2">Images</a></li>
                <li class="gopich"><a href="/Home/Videos" id="navLink3">Videos</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li class="gopich"><a href="#" id="navLink4">Login</a></li>
            </ul>
        </div>
    </nav>
    <div>
        @RenderBody()
    </div>
    <footer>
        <div class="footer">
            <div class="container">
                <div class="row">
                    <h3 style="text-align:left">
                        © Copyright  reserved By STAGCorporation
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        Developed by GopiKrishna Cherukumalli
                    </h3>
                </div>
            </div>
        </div>
    </footer>
    <script type="text/javascript">
        $("li #navLink1").click(function () {
            $(this).parent().addClass('active');
            $('#navLink2').parent().removeClass('active');
            $('#navLink3').parent().removeClass('active');
            $('#navLink4').parent().removeClass('active');
        });
        $("li #navLink2").click(function () {
            $(this).parent().addClass('active');
            $('#navLink1').parent().removeClass('active');
            $('#navLink3').parent().removeClass('active');
            $('#navLink4').parent().removeClass('active');
        });
        $("li #navLink3").click(function () {
            $(this).parent().addClass('active');
            $('#navLink1').parent().removeClass('active');
            $('#navLink2').parent().removeClass('active');
            $('#navLink4').parent().removeClass('active');
        });
        $("li #navLink4").click(function () {
            $(this).parent().addClass('active');
            $('#navLink2').parent().removeClass('active');
            $('#navLink3').parent().removeClass('active');
            $('#navLink1').parent().removeClass('active');
        });
    </script>
</body>
</html>
